  width: 50px;
  height: 50px;
  font-size: 1.5rem;
  color: #333;
  cursor: pointer;
  z-index: 10;
  transition: all 0.3s ease;
}

.slider-nav:hover {
  background: white;
  transform: translateY(-50%) scale(1.1);
}

