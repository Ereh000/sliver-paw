    font-size: 1.2rem;
    transition: transform 0.3s;
  }

  .faq-question.active::after {
    content: '-';
  }

  .faq-answer {
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.4s ease;
    padding: 0 1rem;
    font-size: 0.95rem;
    color: #444;
    line-height: 1.6;
  }

  .faq-answer.open {
    padding: 1rem;
    max-height: 500px;
